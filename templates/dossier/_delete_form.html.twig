<form method="post" action="{{ path('app_dossier_delete', {'idUser': dossier.idUser, 'idOffre' : dossier.idOffre}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dossier.idUser) }}">
    <button class="btn">Delete</button>
</form>